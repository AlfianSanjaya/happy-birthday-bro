{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    { "route": "/assets/*", "allowedRoles": ["admin"] },
    { "route": "/*.css", "allowedRoles": ["admin"] },
    { "route": "/*.js", "allowedRoles": ["admin"] },
    {
      "route": "/*",
      "allowedRoles": [ "admin" ]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad"
    }
  }
}